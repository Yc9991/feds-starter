{
  "name": "feds-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.dev vite",
    "dev:o": "npm run odata:gen && dotenv -e .env.dev vite",
    "dev:h": "dotenv -e .env.dev vite -- --host",
    "build": "vue-tsc && vite build --emptyOutDir",
    "build:w": "vue-tsc && vite build --watch --emptyOutDir",
    "odata:gen": "dotenv -e .env.dev odata2ts",
    "preview": "vite preview",
    "preview:h": "vite preview --host",
    "type-check": "vue-tsc --build"
  },
